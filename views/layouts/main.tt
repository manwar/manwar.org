    <script type="text/javascript">
        $("#contact-form").submit(function(event) {
            /* Get form data */
            var $form   = $(this);
            var name    = $form.find('input[name="user-name"]').val();
            var email   = $form.find('input[name="user-email"]').val();
            var subject = $form.find('input[name="user-email-subject"]').val();
            var message = $form.find('textarea[name="user-message"]').val();
            var url     = $form.attr('action');

            /* Sending the data using post */
            var posting = $.post(url, {
                uname: name,
                uemail: email,
                usubject: subject,
                umessage: message
            });

            /* Display the response */
            posting.done(function(data) {
                $("#contact-response").empty().append(data);;
            });

            //$.post(url, $("#contact-form").serialize()).done(function(data) { $("#contact-response").empty().append(data); });
        });
    </script>
